{
  "name": "denoify",
  "version": "0.1.0",
  "description": "For NPM module authors that would like to support Deno but do not want to write and maintain a port.",
  "repository": {
    "type": "git",
    "url": "git://github.com/garronej/denoify"
  },
  "scripts": {
    "tsc": "npx tsc && chmod +x ./dist/bin/denoify.js",
    "test": "node ./dist/test/main"
  },
  "bin": {
    "denoify": "./dist/bin/denoify.js"
  },
  "author": "Joseph Garrone",
  "license": "MIT",
  "dependencies": {
    "@types/comment-json": "^1.1.1",
    "commander": "^4.1.1",
    "comment-json": "^3.0.2",
    "node-fetch": "^2.6.0",
    "scripting-tools": "github:garronej/scripting-tools",
    "url-join": "^4.0.1"
  },
  "keywords": [
    "deno",
    "transpile",
    "cjs",
    "CommonJs"
  ],
  "devDependencies": {
    "js-yaml": "^3.13.1",
    "@types/js-yaml": "^3.12.3",
    "@types/node": "^8.10.59",
    "@types/node-fetch": "^2.5.6",
    "@types/url-join": "^4.0.0",
    "typescript": "^3.7.5"
  }
}
